{
  "manifest_version": 3,
  "name": "SEO Analyzer Tools by SEEDO Media",
  "version": "1.0.0",
  "description": "Comprehensive SEO analysis with issues detection and data export tools. Based on the excellent SEO Analyzer by Slim SEO.",
  "author": "SEEDO Media",
  "homepage_url": "https://seedo.media",
  "browser_specific_settings": {
    "gecko": {
      "id": "seatools@seedo.media",
      "data_collection_permissions": {
        "optional": []
      }
    }
  },
  "icons": {
    "16": "icons/icon-16.png",
    "32": "icons/icon-32.png",
    "48": "icons/icon-48.png",
    "64": "icons/icon-64.png",
    "96": "icons/icon-96.png",
    "128": "icons/icon-128.png"
  },
  "permissions": [
    "scripting",
    "activeTab",
    "storage"
  ],
  "action": {
    "default_title": "SEO Analyzer Tools by SEEDO Media",
    "default_icon": {
      "16": "icons/icon-16.png",
      "32": "icons/icon-32.png",
      "48": "icons/icon-48.png",
      "64": "icons/icon-64.png",
      "96": "icons/icon-96.png",
      "128": "icons/icon-128.png"
    }
  },
  "background": {
    "scripts": [
      "ExtPay.js",
      "background.js"
    ]
  }
}